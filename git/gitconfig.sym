[user]
	name = Nathaniel Rubin
	email = nateboop@gmail.com
[core]
	askpass = git-gui--askpass
	excludesfile = ~/.gitignore
  editor = /usr/local/Cellar/macvim/7.4-104/MacVim.app/Contents/MacOS/Vim
[merge]
	tool = vimdiff
  conflictstyle = diff3
  prompt = false
[color]
	ui = auto
	status = auto
[filter "hawser"]
	clean = git hawser clean %f
	smudge = git hawser smudge %f
	required = true
[push]
	default = current
[commit]
    template = ~/.gitmessage
